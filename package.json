{
  "name": "playground",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/playground/playground.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build-doc": "stencil build --docs-readme",
    "build:storybook": "build-storybook -c .storybook -s www -o .storybook-build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006 -c .storybook -s www",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@stencil/core": "^1.3.0",
    "@storybook/html": "^5.1.11",
    "@storybook/polymer": "^4.0.0-alpha.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "lit-html": "^1.1.2",
    "polymer-webpack-loader": "^2.0.3"
  },
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.4"
  }
}
